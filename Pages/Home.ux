<Page ux:Class="Home">
	<Router ux:Dependency="router" />
	<JavaScript>
		function addSession() {
			router.push("AddSession");
		}
		function goToSession() {
			router.push("Session");
		}
		module.exports={
			addSession,
			goToSession
		}
	</JavaScript>
	<DockPanel>
		<Grid Dock="Top" Background="#252e3e" MinHeight="50" Columns="1*,5*,1*">
			<Components.HumbMenu  />
			<myText Font="TheSansBold" TextAlignment="Center" Alignment="Center" TextColor="#fff">Home</myText>
			<!-- <myText TextAlignment="Center" Alignment="Center"></myText> -->
			<Panel></Panel>
		</Grid>
		<ScrollView Dock="Fill">
			<StackPanel Padding="0,0,0,0">
				<Each Count="10">
					<Components.ItemMenu Clicked="{goToSession}" />
				</Each>
			</StackPanel>
		</ScrollView>
		<Components.FloatButton Value="+" Clicked="{addSession}" />
	</DockPanel>

</Page>
